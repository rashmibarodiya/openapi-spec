{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.ts",
    "dist/routes.ts"  
  ],
  "exclude": ["node_modules","dist"]
}
